{
  "presets": [
    "env",
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "react-hot-loader/babel"
  ],
  "env": {
    "production": {
      "plugins": [
        ["transform-react-remove-prop-types", {
            "removeImport": true
        }]
      ]
    },
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  }
}
